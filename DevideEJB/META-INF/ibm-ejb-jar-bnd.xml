<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar-bnd
        xmlns="http://websphere.ibm.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-ejb-jar-bnd_1_0.xsd"
        version="1.0">

    <!--  Can be looked up by "ejblocal:nab/devideService"    (Returns local interface)                   -->
    <!--  Can be looked up by          "nab/devideService"    (Returns Remote interface)                  -->

    <!--  Value for "name" attribute of <session> must match the one defined in @EJB(beanName = "AddServiceEJB"  -->
    <session name="MyDevideBean" simple-binding-name="nab/devideService"/>
</ejb-jar-bnd>


